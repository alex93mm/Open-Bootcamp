{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "settings":{
        "react":{
            "version": "detect"
        }

    },
    "overrides": [
        {
            "files": [
                "*.jsx",
                "*.js"
            ]
        }
    ],
    "globals": {
        "document": true
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "exclude" : [],
    "rules": {
        "indent": 0,
        "react/prop-types": 0,
        "arrow-body-style": 0,
        "linebreak-style": 0
    }
}
